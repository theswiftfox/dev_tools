{{#each this}}
<div data-id="{{this.id}}" data-creator="{{this.creator}}" class="note w3-card w3-yellow w3-padding">
    <div class="w3-container w3-border-bottom" style="min-height: 25px;" contenteditable="true">
        {{this.title}}
    </div>
    <div class="w3-container w3-padding" contenteditable="true">
        {{this.content}}
    </div>
</div>
<p></p>
{{/each}}